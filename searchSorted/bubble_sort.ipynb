   "source": [
    "#bubble sort - time O(n2) space O(1)\n",
    "nums = [1, 9, 7, 4, 2]\n",
    "size_nums = len(nums)\n",
    "\n",
    "for i in range(size_nums - 1):\n",
    "    for j in range(size_nums - i - 1):\n",
    "        if nums[j] > nums[j+1]:\n",
    "            nums[j], nums[j+1] = nums[j+1], nums[j]\n",
    "print(nums) "
   ]
  }
